{
  "name": "semester-project-2",
  "version": "0.0.1",
  "description": "semester project 2",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pjatte1337/semester-project-2.git"
  },
  "keywords": [
    "semester",
    "project"
  ],
  "author": "Joakim Vanebo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pjatte1337/semester-project-2/issues"
  },
  "homepage": "https://github.com/Pjatte1337/semester-project-2#readme",
  "devDependencies": {
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "jest": "^29.2.0",
    "lint-staged": "^13.1.0",
    "live-server": "^1.2.2",
    "prettier": "^2.8.0",
    "sass": "^1.56.1"
  },
  "dependencies": {
    "bootstrap": "^5.2.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
